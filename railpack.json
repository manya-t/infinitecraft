{
  "$schema": "https://schema.railpack.com",
  "steps": {
    "install": {
      "commands": [
        "apt-get update",
        "apt-get install -y graphviz",
        "python manage.py makemigrations",
        "python manage.py migrate"
      ]
    }
  },
  "deploy": {
    "startCommand": "gunicorn infinitecraft.wsgi"
  }
}